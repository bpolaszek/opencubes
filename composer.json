{
  "name": "bentools/opencubes",
  "type": "library",
  "description": "",
  "license": "MIT",
  "require": {
    "php": ">=7.1.3",
    "ext-json": "*",
    "bentools/iterable-functions": "~1.2",
    "bentools/querystring": "~1.0,>=1.0.2",
    "bentools/uri-factory": "~1.1|~2.0",
    "symfony/options-resolver": "~2.6|~3.0|~4.0|~5.0"
  },
  "require-dev": {
    "filp/whoops": "^2.3",
    "guzzlehttp/psr7": "^1.4",
    "phpunit/phpunit": "^7.1",
    "php-coveralls/php-coveralls": "^2.1",
    "squizlabs/php_codesniffer": "^3.4",
    "symfony/var-dumper": "^4.0"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "BenTools\\OpenCubes\\": "src/"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "BenTools\\OpenCubes\\Tests\\": "tests/"
    },
    "files": [
      "vendor/symfony/var-dumper/Resources/functions/dump.php"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
